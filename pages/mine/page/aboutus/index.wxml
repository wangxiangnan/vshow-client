<view class="extrafns">
  <view class="weui_panel relative bgf">
    <view class="weui_panel_hd">关于我们</view>

    <view class="weui_panel_bd">
      <view class="weui_media_box" wx:for="{{ fnList }}" wx:key="{{ item.id }}" id="{{ item.id }}" bindtap="tapMedia">
        <view class="weui_media_title">{{ item.title }}</view>

        <view class="weui_media_desc">{{ item.desc }}</view>
      </view>
    </view>

  </view>

</view>