<view class="result">
  <view class="result_bg" style="background-image: url({{ customer.avatarUrl }})"></view>
  <view class="result_bd">
    <view class="pairs">
      <view class="oneself avatar" style="background-image: url({{ user.avatarUrl }})"></view>
      <view class="other avatar" style="background-image: url({{ customer.avatarUrl }})"></view>
    </view>

    <view class="Pair_text">
      <text>恭喜你和{{ customer.nickName }}互相欣赏!</text>
    </view>

    <view class="chat_btn" bindtap="beginChat">发送消息</view>
    <view class="swipe" bindtap="beginSwipe">继续左滑右滑</view>
  </view>
</view>
